<Page x:Class="hciProjekat.PredmetiPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:hciProjekat"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="PredmetiPage">

    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" HorizontalAlignment="Stretch" VerticalAlignment="Top">
            <Button Content="Ucionice" Click="UcioniceBtnClick"></Button>
            <Button Content="Predmeti" Margin="10,0,0,0" IsEnabled="False"></Button>
            <Button Content="Smjerovi" Margin="10,0,0,0" Click="SmjeroviBtnClick"></Button>
            <Button Content="Softver" Margin="10,0,0,0" Click="SoftverBtnClick"></Button>
        </StackPanel>
    </Grid>
</Page>
